{
  "name": "@rimori/client",
  "version": "1.3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rimori-release": "./dist/cli/scripts/release/release.js",
    "rimori-init": "./dist/cli/scripts/init/main.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/core.d.ts",
      "default": "./dist/core/core.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "default": "./dist/components.js"
    }
  },
  "scripts": {
    "build": "tsc && sass src/style.scss:dist/style.css",
    "dev": "tsc -w  --preserveWatchOutput",
    "css-dev": "sass --watch src/style.scss:dist/style.css"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.48.1",
    "@tiptap/react": "2.10.3",
    "@tiptap/starter-kit": "2.10.3",
    "html2canvas": "^1.4.1",
    "react-icons": "^5.4.0",
    "react-markdown": "^10.1.0",
    "tiptap-markdown": "^0.8.10"
  },
  "devDependencies": {
    "dotenv": "^16.5.0",
    "form-data": "^4.0.2",
    "node-fetch": "^3.3.2",
    "sass": "^1.82.0",
    "typescript": "^5.7.2"
  }
}
